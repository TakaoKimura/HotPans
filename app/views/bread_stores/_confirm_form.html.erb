<%= form_for :bread_store, url: getFormActionURL(current_view), method: getFormMethod(current_view) do |f| %>
	<%= f.hidden_field :name %>
	<%= f.hidden_field :phone_number %>
	<%= f.hidden_field :address %>
	<%= f.submit "確定", class: "btn btn-default"%>
	<%= button_tag "戻る", :type => "button", :onclick => "window.history.back();"%>
<% end %>